      function reload_list(params){
        params_for_export = []
        for (i=0; i<$('input.search').length; i++){
          input_search = $('input.search')[i]
          if (input_search.value != ''){
            params_for_export.push('search['+input_search.name+']' + '=' + input_search.value)
            params['search['+input_search.name+']'] = input_search.value
          }
        }
        if($("#store").length > 0){
          params['store'] = $("#store").val()}
        if($("#start_date").length > 0){
          params['start_date'] = $("#start_date").val()}
        if($("#end_date").length > 0){
          params['end_date'] = $("#end_date").val()}
        $.ajax({
          url: '<%= request.env['REQUEST_PATH'] %>'.split('?page')[0],
          method: "get",
          cache: false,
          data: params,
          dataType: "script"
        })
        $('#export_to_csv').attr('href', "/<%= params[:controller] %>.csv?locale=en&"+params_for_export.join('&'))
        $('#export_to_csv3').attr('href', "/<%= params[:controller] %>.csv?locale=en&store="+($('#store').length == 1 ? $('#store').val() : '')+"&"+params_for_export.join('&'))
        $('#export_to_xls').attr('href', "/<%= params[:controller] %>.xls?locale=en&"+params_for_export.join('&'))
        $('#export_to_xlsx').attr('href', "/<%= params[:controller] %>.xlsx?locale=en&"+params_for_export.join('&'))
        $('#export_to_xls2').attr('href', "/"+ $("#eksport").val() +".xls?locale=en&"+params_for_export.join('&'))
        $('#export_to_xls3').attr('href', "/<%= params[:controller] %>.xls?locale=en&start_date="+($('#start_date').length == 1 ? $('#start_date').val() : '')+"&store="+($('#store').length == 1 ? $('#store').val() : '')+"&end_date="+($('#end_date').length == 1 ? $('#end_date').val() : '')+"&"+params_for_export.join('&'))
        $('.print_newest_product').attr('href', "/price_tags/print_newest_product?"+params_for_export.join('&'))
      }

      <% if params[:controller] == 'dashboards' %>
        function demoLoadAttribute(data) {/*jslint smarttabs:true */
          var latlngs = {};
          var markers = []
          var infowindow = []
          var images = []
          var centerLatLng = new google.maps.LatLng(1, 1),
          mapOptions = {
            mapTypeId : google.maps.MapTypeId.TERRAIN, mapTypeControlOptions: {mapTypeIds: [google.maps.MapTypeId.TERRAIN, google.maps.MapTypeId.ROADMAP, google.maps.MapTypeId.SATELLITE]}
          },
          bounds = new google.maps.LatLngBounds(),
          map = new google.maps.Map(document.getElementById("map_canvas"), mapOptions);
          for (var i=0; i<data.length; i++){
            if (data[i].achievement<=50){
              images.push({
                url: '/img/markers/merah.png', size: new google.maps.Size(27, 33),// This marker is 20 pixels wide by 32 pixels tall.
                origin: new google.maps.Point(0,0),// The origin for this image is 0,0.
                anchor: new google.maps.Point(0, 33)// The anchor for this image is the base of the flagpole at 0,32.
              });
            }
            else if (data[i].achievement<=70){
              images.push({
                url: '/img/markers/kuning.png', size: new google.maps.Size(27, 33),// This marker is 20 pixels wide by 32 pixels tall.
                origin: new google.maps.Point(0,0),// The origin for this image is 0,0.
                anchor: new google.maps.Point(0, 33)// The anchor for this image is the base of the flagpole at 0,32.
              });
            }
            else{
              images.push({
                url: '/img/markers/hijau.png', size: new google.maps.Size(27, 33),// This marker is 20 pixels wide by 32 pixels tall.
                origin: new google.maps.Point(0,0),// The origin for this image is 0,0.
                anchor: new google.maps.Point(0, 33)// The anchor for this image is the base of the flagpole at 0,32.
              });
            }
            var outletId = "outlet_" + data[i].id
            if (latlngs[outletId] == undefined)
              latlngs[outletId] = []
            latlng = new google.maps.LatLng(data[i].lat, data[i].long)
            latlngs[outletId].push(new google.maps.LatLng(data[i].lat, data[i].long));
            bounds.extend(new google.maps.LatLng(data[i].lat, data[i].long));
            markers.push(new google.maps.Marker({icon: images[i], position: latlng, map: map}))
            infowindow.push(new google.maps.InfoWindow({
              content: '<div id="content"><div id="bodyContent"><p>Achievement'+' : '+data[i].achievement+'%</p><p>Store : '+data[i].office_name+'</p><p>Target : '+data[i].target_sec2015
                +'</p><p>Sales : '+data[i].secsales+'</p></div></div>'
            }));
            google.maps.event.addListener(markers[i], 'mouseover', (function(markers, i) {
              return function() {
                for (var idx_infowindow = 0; idx_infowindow<infowindow.length; idx_infowindow++)
                  infowindow[idx_infowindow].close()
                infowindow[i].open(map, markers[i]);
              }
            })(markers, i));
          }
          map.fitBounds(bounds);
        }
