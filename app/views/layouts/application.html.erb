  function chooseSupplier(id) {
    var supplier_name = $('#supplier-' + id + ' td:nth-child(3)').text();
    $('.supl_code').html($('#supplier-' + id + ' td:nth-child(2)').text());
    $('.supl_code2').val($('#supplier-' + id + ' td:nth-child(3)').text());
    $('.supl_name').html($('#supplier-' + id + ' td:nth-child(3)').text());
    $('.supl_address').html($('#supplier-' + id + ' td:nth-child(4)').text());
    $('#purchase_order_top').val($('#supplier-' + id + ' td:nth-child(6)').text());
    $('.supplier_id').val(id)
    $('#supplier').val(supplier_name);
    <% if params[:controller] == 'returns_to_suppliers' %>
      udah_keisi()
    <% else %>
      $('.browser_product').attr('disabled', false)
    <% end %>
    data_terisi_semua()
  }

  function data_terisi_semua(){
    if($("#purchase_order_head_office_id").val() != ""){
      $('#download-template').removeClass('disabled');
      $('#download-template').prop('disabled', false);
      if($("#import_excel").val() != '' && $("#purchase_order_top").val() != "" && $("#purchase_order_supplier_id").val() != ""){
        $(".tombol-export").prop('disabled', false);
      }
      else{
        $(".tombol-export").prop('disabled', true);
      }
    }
    else{
      $(".tombol-export").prop('disabled', true);
      $('#download-template').addClass('disabled');
      $('#download-template').prop('disabled', true);
    }
  }

