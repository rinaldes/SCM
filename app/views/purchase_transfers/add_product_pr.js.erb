$('#pr_date').val('<%= @purchase_request.date.strftime("%d-%m-%Y") %>')
$('#pr_branch').val('<%= @purchase_request.branch.office_name %>')
$('#pr_qty').val('<%= @purchase_request.purchase_request_details.map(&:approved_qty).compact.sum %>')
$('#ref_transaction').val(($('#ref_transaction').val() == '' ? "'" + '<%= params[:number] %>' : $('#ref_transaction').val()+", '"+ui.item.value) + "'")
$('#list_detail').append("<%= j(render 'list_detail', key: params[:key], details: @purchase_request.purchase_request_details, pr_id: @purchase_request.id) %>")
$('#product_detail').append("<%= j(render 'product_detail', key: params[:key], pr_id: @purchase_request.id) %>")
clone_details()
$('#purchase_transfer_destination_warehouse_id').val('<%= @purchase_request.branch_id %>')
$('#supplier_code').val('<%= @supplier.code rescue '-' %>')
$('#supplier_name').val('<%= @supplier.name rescue '-' %>')
$('#alamat').val('<%= @supplier.address rescue '-' %>')
$('.supplier_id').val('<%= @supplier.id rescue '-' %>')