<div class="container-fluid">
  <ul class="breadcrumb">
        <li><p>YOU ARE HERE</p></li> 
        <li><a href="#" class="active">Visi Misi</a></li>
    </ul>
    
    <div class="page-title">  
      <i class="fa fa-eye"></i>
      <h3><span class="semi-bold">Visi</span> &amp; <span class="semi-bold">Misi</span> </h3> 
    </div>  
    <div class="grid simple col-xs-9">
        <div class="panel-group" id="accordion" data-toggle="collapse">
          <div class="panel panel-default">
            <div class="panel-heading">
              <h4 class="panel-title">
                <a class="collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                   Visi
                </a>
              </h4>
            </div>
            <div id="collapseOne" class="panel-collapse expand in grid simple">
              <article class="panel-body grid-body no-border m-t-10">
                 <br />
                 <p>
                  <%= @visi.content_ind.html_safe %>
                 </p>
                 
              </article>
            </div>
          </div>
          <div class="panel panel-default">
            <div class="panel-heading">
              <h4 class="panel-title">
                <a class="" data-toggle="collapse" data-parent="#accordion"  href="#collapseTwo">
                  Misi
                </a>
              </h4>
            </div>
            <div id="collapseTwo" class="panel-collapse expand in grid simple">
              <article class="panel-body grid-body no-border m-t-10">       
                 <br />
                 <p>
                  <%= @misi.content_ind.html_safe %>
                 </p>
              </article>
            </div>
          </div>
          
        </div> 
    </div>

    <div class="grid simple col-xs-9">  
      <%= link_to dashboards_path, :class => 'btn btn-success' do %>
          <span class="fa fa fa-arrow-circle-o-left"></span> Next
      <% end %>
    </div>

</div>