<% if @stock_opname.present? %>
  $("#so_form").html("<%= j(render 'so_form', stock_opname: @stock_opname) %>")
  setTimeout(function() {
    window.location = '/stock_opnames/<%= @stock_opname.id %>/edit'
  }, 1000);
<% else %>
  if ($('#stock_opname_office_id').val() != '')
    $("#start-now").attr("disabled", false)
<% end %>
