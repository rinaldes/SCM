<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">Receive No</Data></Cell>
        <Cell><Data ss:Type="String">Date</Data></Cell>
        <Cell><Data ss:Type="String">Supplier</Data></Cell>
        <Cell><Data ss:Type="String">PO Number</Data></Cell>
        <Cell><Data ss:Type="String">Received Qty</Data></Cell>
        <Cell><Data ss:Type="String">PO Qty</Data></Cell>
        <Cell><Data ss:Type="String">Outstanding Qty</Data></Cell>
        <Cell><Data ss:Type="String">Total Cost Price</Data></Cell>
        <Cell><Data ss:Type="String">Note</Data></Cell>
        <Cell><Data ss:Type="String">Status</Data></Cell>

      </Row>
    <% Receiving.all.each do |receiving| %>
      <Row>
        <Cell><Data ss:Type="String"><%= receiving.number %></Data></Cell>
        <Cell><Data ss:Type="String"><%= receiving.date rescue '' %></Data></Cell>
        <Cell><Data ss:Type="String"><%= receiving.supplier.name rescue '' %></Data></Cell>
        <Cell><Data ss:Type="String"><%= receiving.purchase_order.number rescue '' %></Data></Cell>
        <Cell><Data ss:Type="String"><%= receiving.received_qty rescue '' %></Data></Cell>
        <Cell><Data ss:Type="String"><%= receiving.purchase_order.total_qty rescue '' %></Data></Cell>
        <Cell><Data ss:Type="String"><%= receiving.outstanding_qty rescue '' %></Data></Cell>
        <Cell><Data ss:Type="String"><%= receiving.total rescue '' %></Data></Cell>
        <Cell><Data ss:Type="String"><%= receiving.note rescue '' rescue '' %></Data></Cell>
        <Cell><Data ss:Type="String"><%= receiving.status rescue '' %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>